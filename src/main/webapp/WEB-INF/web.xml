<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>

    <display-name>RSR</display-name>
    
    <servlet>
        <servlet-name>rsr</servlet-name>
        <servlet-class>cz.muni.kisk.rsr.RSRServlet</servlet-class>
        <init-param>
            <param-name>param</param-name>
            <param-value>value</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>rsr</servlet-name>
        <url-pattern>/rsr/*</url-pattern>
    </servlet-mapping>

</web-app>
